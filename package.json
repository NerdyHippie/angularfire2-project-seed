{
  "name": "angularfire2-project-seed",
  "version": "0.0.2",
  "description": "starter project using angular-cli and Firebase Database and Hosting",
  "license": "MIT",
  "angular-cli": {},
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "lint": "tslint \"src/**/*.ts\" --project src/tsconfig.json --type-check && tslint \"e2e/**/*.ts\" --project e2e/tsconfig.json --type-check",
    "test": "ng test",
    "pree2e": "webdriver-manager update --standalone false --gecko false",
    "e2e": "protractor",
    "build-prod": "ng build --prod",
    "build-dev": "ng build --dev",
    "prod-build-and-deploy": "ng build --prod && firebase deploy --project prod",
    "dev-build-and-deploy": "ng build --dev && firebase deploy --project dev"
  },
  "private": true,
  "dependencies": {
